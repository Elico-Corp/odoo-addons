  <story>
    <para style="terp_default_8">[[repeatIn(objects,'o')]]</para>
    <para style="terp_default_8">[[ setLang( 'en_US') ]]</para>
    <para style="terp_default_8">
      <font color="white"> </font>
    </para>
    <para style="P24">[[ (o.state=='draft' or o.state=='sent') and removeParentNode('para') ]] <font face="Helvetica">Purchase Application /采购申请单： </font>[[ o.name ]]</para>
    <para style="P24">
      <font face="Helvetica">[[ o.state&lt;&gt;'draft' and o.state&lt;&gt;'sent' and removeParentNode('para') ]] Purchase Application /采购申请单： [[ o.name ]]</font>
    </para>
    <para style="P12">[[ (o.state=='draft' or o.state=='sent') and removeParentNode('para') ]] <font face="Helvetica">PO Application Date/采购申请日期:</font> [[ formatLang(o.date_order,date = True) ]]</para>
    <para style="P12">[[ o.state&lt;&gt;'draft' and o.state&lt;&gt;'sent' and removeParentNode('para') ]] <font face="Helvetica">PO Application Date/采购申请日期：</font> [[ formatLang(o.date_order,date = True) ]]</para>
    <para style="P11">
      <font color="white"> </font>
    </para>
    <para style="P31">Supplier/ 供应商</para>
    <para style="P32">Name/名称：[[ (o.partner_id and o.partner_id.name) or '' ]]</para>
    <para style="P32">City/城市：[[ (o.partner_id and o.partner_id.state_id and o.partner_id.state_id.name) or '']] - [[ (o.partner_id and o.partner_id.city) or '' ]] - [[ (o.partner_id and o.partner_id.zip) or '' ]] </para>
    <para style="P32">Add/地址:[[ (o.partner_id and o.partner_id.street) or '']]</para>
    <para style="P32">Phone/电话: [[ (o.partner_id and o.partner_id.phone) or '']]</para>
    <para style="P32">Fax/传真: [[ (o.partner_id and o.partner_id.fax) or '']]</para>
    <para style="P32">Contact/联系人：[[ o.contact_partner_id and o.contact_partner_id.name or '']]</para>
    <para style="P32">Mobile/手机：[[ o.contact_partner_id and o.contact_partner_id.mobile or '']]</para>
    <para style="P32">Mail/邮件：[[ o.contact_partner_id and o.contact_partner_id.email or '']]</para>
    <para style="P32">
      <font color="white"> </font>
    </para>
    <blockTable colWidths="20.0,184.0,86.0,56.0,57.0,44.0,71.0" repeatRows="1" style="Table5">
      <tr>
        <td>
          <para style="P26">#</para>
        </td>
        <td>
          <para style="P4">[编号] 产品描述</para>
          <para style="P4">[Part number] Product Description</para>
        </td>
        <td>
          <para style="P29">预定日期</para>
          <para style="P8"><font face="Helvetica">Req.</font> Date</para>
        </td>
        <td>
          <para style="P5">数量</para>
          <para style="P5">Quantity</para>
        </td>
        <td>
          <para style="P3">单价</para>
          <para style="P3">Unit Price</para>
        </td>
        <td>
          <para style="P5">增值税</para>
          <para style="P5">VAT</para>
        </td>
        <td>
          <para style="P25">累计发货额</para>
          <para style="P25">Total Amount</para>
        </td>
      </tr>
    </blockTable>
    <section>
      <para style="terp_default_8">[[repeatIn(o.order_line,'line')]]</para>
      <blockTable colWidths="20.0,184.0,86.0,57.0,57.0,44.0,72.0" style="Table_Order_Pur_line_Content">
        <tr>
          <td>
            <para style="P14">[[ o.order_line.index(line) + 1]]</para>
          </td>
          <td>
            <para style="P14">[[ get_product_desc(line) or removeParentNode('para') ]]</para>
            <para style="P14">[[ get_product_desc_en(line) or removeParentNode('para') ]]</para>
          </td>
          <td>
            <para style="P18">[[ formatLang( line.date_planned, date=True) ]]</para>
          </td>
          <td>
            <para style="P27">[[ formatLang(line.product_qty, digits=get_digits(dp='Product UoM') ) ]] [[ get_product_unit(line) ]]</para>
          </td>
          <td>
            <para style="P27">[[ formatLang(line.price_unit, digits=get_digits(dp='Purchase Price') ) ]]<font face="Helvetica">[[ o.pricelist_id.currency_id. symbol ]]</font></para>
          </td>
          <td>
            <para style="P13">[[ ', '.join(map(lambda x: x.name, line.taxes_id)) ]]</para>
          </td>
          <td>
            <para style="P28">[[ formatLang(multiply(line.product_qty,line.price_unit), digits=get_digits(dp='Account') ) ]]<font face="Helvetica">[[ o.pricelist_id.currency_id. symbol ]]</font></para>
          </td>
        </tr>
      </blockTable>
    </section>
    <blockTable colWidths="289.0,114.0,115.0" style="Table_All_Total_Detail">
